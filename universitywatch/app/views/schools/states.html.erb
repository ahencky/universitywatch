<html>
<head>
  <title>State search</title>
  <script src="//cdnjs.cloudflare.com/ajax/libs/d3/3.5.3/d3.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/topojson/1.6.9/topojson.min.js"></script>
  <script src="http://datamaps.github.io/scripts/datamaps.usa.min.js"></script>
  <div id="container" style="position: relative; width: 100%; height: 500px;"></div>
  <script>
      var map = new Datamap({
        element: document.getElementById('container'),
        scope: 'usa'
      });
  </script>
</head>
<body>
  <div id ="container">

  </div>


    <table class="table" id="school_table">
      <tr>
        <th>School Name</th>
      </tr>

      <script id="table_generator" type="text/x-handlebars-template">
      {{#each state_info}}
      <tr>
      <td>{{name}}</td>
      </tr>
      {{/each}}
      </script>



    </table>
</body>
</html>
